<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone Number</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= button_to 'Call', call_user_path(user), remote: true, method: 'POST' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
